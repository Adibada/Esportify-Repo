<IfModule mod_rewrite.c>
    RewriteEngine On

    # Si le fichier ou dossier existe, on le sert tel quel
    RewriteCond %{REQUEST_FILENAME} -f [OR]
    RewriteCond %{REQUEST_FILENAME} -d
    RewriteRule ^ - [L]

    # Sinon, on laisse Symfony g√©rer via index.php
    RewriteRule ^ index.php [L]
</IfModule>
